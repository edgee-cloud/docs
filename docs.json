{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Edgee documentation",
  "description": "Edgee is an edge-native AI Gateway that reduces LLM costs by up to 50% through token compression and intelligent routing.",
  "colors": {
    "primary": "#8924A6",
    "light": "#C876FA",
    "dark": "#8924A6"
  },
  "favicon": "/favicon.svg",
  "icons": {
    "library": "lucide"
  },
  "redirects": [
    {
      "source": "/components/*",
      "destination": "/proxy/components/*"
    },
    {
      "source": "/services/*",
      "destination": "/proxy/services/*"
    },
    {
      "source": "/getting-started/*",
      "destination": "/proxy/getting-started/*"
    },
    {
      "source": "/introduction/architecture",
      "destination": "/introduction/why-edgee"
    },
    {
      "source": "/introduction/network-map",
      "destination": "/introduction/why-edgee"
    },
    {
      "source": "/introduction/privacy",
      "destination": "/introduction/why-edgee"
    }
  ],

  "fonts": {
    "heading": {
      "family": "Sentient-Variable",
      "source": "https://www.edgee.ai/assets/fonts/Sentient-Variable.woff2",
      "format": "woff2",
      "weight": 600
    },
    "body": {
      "family": "Supreme-Variable",
      "source": "https://www.edgee.ai/assets/fonts/Supreme-Variable.woff2",
      "format": "woff2",
      "weight": 500
    }
  },
